
import java.util.*;
class Main {
    public static void main(String[] args) {
        Map<Character,Integer>mp=new HashMap<>();
        String s="aabcbcdc";
        for(int i=0;i<s.length();i++)
        {
            char ch=s.charAt(i);
            if(!mp.containsKey(ch))
            {
                mp.put(ch,1);
            }
            else
            {
                mp.put(s.charAt(i),mp.get(ch)+1);
            }
        }
        System.out.println(mp.entrySet());
        int max=Integer.MAX_VALUE;
        int count=0;
        char alpha=' ';
        for(var j:mp.entrySet())
        {
            if(j.getValue()<max)
            {
                max=j.getValue();
                alpha=j.getKey();
            }
        }
         System.out.println("The character "+alpha+" has occured "+max+" time");
         
    }
}
