
import java.util.*;
class Main {
    public static void main(String[] args) {
        boolean palindrome=false;
        String s="madam";
        int i=0,j=s.length()-1;
        while(i<j)
        {
            if(s.charAt(i)!=s.charAt(j))
            {
                palindrome=true;
                System.out.println("it is not a palindrome");
                break;
            }
            i++;
            j--;
        }
        if(palindrome==false)
        {
            System.out.println("it is a palindrome");
        }
        
    }
}
